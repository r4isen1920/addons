  "origin_trait_container_template": {
    "type": "panel",
    "size": [ "100%", "100%c - 3px" ],
    "clips_children": true,
    
    "$trait_name|default": "trait_name",
    "$trait_type|default": "plus",
    "$trait_description|default": "trait_description",
    "controls": [
      { 
        "scroller_control@common.button": {
          "size": [ "100%", 1 ],
          "sound_volume": 0.0,
          "$pressed_button_name": "unset"
        } 
      },
      {
        "trait_type": {
          "type": "image",
          "layer": 1,
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [ 0, 3 ],
          "size": [ 25, 11 ],
          "texture": "('textures/origins/traits/' + $trait_type)"
        }
      },
      {
        "content_title": {
          "type": "label",
          "color": "$body_text_color",
          "layer": 1,
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [ 26, 4 ],
          "size": [ "100% - 26px", "default" ],
          "shadow": true,
          "text": "$trait_name",
          "localize": true
        }
      },
      {
        "content_description": {
          "type": "label",
          "color": "$7_color_format",
          "layer": 1,
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [ "2% + 2px", 16 ],
          "size": [ "98% - 2px", "default" ],
          "shadow": true,
          "text": "$trait_description",
          "localize": true
        }
      }
    ]
  }
}