  "advancement_items_index@common.empty_panel": {
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "story_tab@advancement_items_story_tab": {
          "ignored": "(($var_title_text - 'story_tab:') = $var_title_text)"
        }
      },
      {
        "adventure_tab@advancement_vertical_scrollable": {
          "$scrolling_content": "advancements_screen_content.advancement_items_adventure_tab",
          "ignored": "(($var_title_text - 'adventure_tab:') = $var_title_text)"
        }
      },
      {
        "nether_tab@advancement_vertical_scrollable": {
          "$scrolling_content": "advancements_screen_content.advancement_items_nether_tab",
          "ignored": "(($var_title_text - 'nether_tab:') = $var_title_text)"
        }
      },
      {
        "the_end_tab@advancement_vertical_scrollable": {
          "$scrolling_content": "advancements_screen_content.advancement_items_the_end_tab",
          "ignored": "(($var_title_text - 'the_end_tab:') = $var_title_text)"
        }
      },
      {
        "husbandry_tab@advancement_vertical_scrollable": {
          "$scrolling_content": "advancements_screen_content.advancement_items_husbandry_tab",
          "ignored": "(($var_title_text - 'husbandry_tab:') = $var_title_text)"
        }
      },
      {
        "legacy_java_achievements_tab@advancement_vertical_scrollable": {
          "$scrolling_content": "advancements_screen_content.legacy_java_achievements_tab",
          "$scrolling_pane_offset": [ 0, 1 ],
          "ignored": "(($var_title_text - 'legacy_tab:') = $var_title_text)"
        }
      }
    ],
    "ignored": "(not (('%.6s' * $var_title_text) = '_r4ui:'))"
  },

