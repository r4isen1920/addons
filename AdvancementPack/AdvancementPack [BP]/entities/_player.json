      "is_experimental": false,
      "scripts": {
        "animate":[
          "dynamic_load", // [Root] Runs commands on first tick
          "runtimes", // [Root] Runs commands every tick
          "item", // [Detect] Item queries
          "armor", // [Detect] Queries worn equipment(s)
          "ranged", // [Detect] Item queries for 'chargeable' items
          "netherWalk", // [Detect] Counts distance difference every tick when the player entered the nether portal
          "biomes", // [Detect] Checks biome tags the player is in, sorts them into biomes, and saves it as a tag
          "shulkerY", // [Detect] Counts distance difference every tick as soon as the player levitates off the ground
          "sleep", // [Detect] Queries if the player is sleeping
          "consumeFood", // [Detect] Item queries for food items
          "consumeTime", // [Detect] Checks if the food item is fully consumed
          "minecart" // [Detect] Detects for how far the player has traveled by minecart
        ]
        /*
        "initialize":[
          // Dummy Components: [q.is_baby -> is_in_nether] | [q.is_chested -> is_riding_minecart]
          "v.x_position = (q.is_baby ? q.position(0) : 0.0);",
          "v.y_position = (q.is_levitating ? q.position(1) : 0.0);",
          "v.z_position = (q.is_baby ? q.position(2) : 0.0);",
